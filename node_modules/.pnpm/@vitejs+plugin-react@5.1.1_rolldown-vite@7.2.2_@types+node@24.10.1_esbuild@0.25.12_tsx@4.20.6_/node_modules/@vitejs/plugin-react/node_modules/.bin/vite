#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/bin/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/bin/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules:/Users/hoi/Documents/work/ecommerce-ai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../rolldown-vite@7.2.2_@types+node@24.10.1_esbuild@0.25.12_tsx@4.20.6/node_modules/rolldown-vite/bin/vite.js" "$@"
fi
